    <DataSet Name="PostingUserNameList">
      <Query>
        <DataSourceName>SqlServerPortalDw</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@fromDate">
            <Value>=Parameters!fromDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@toDate">
            <Value>=Parameters!toDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT DISTINCT(userName) AS userName
FROM MasterCustomerPosting 
WHERE
CAST(MasterCustomerPosting.createdAt AS date) &gt;= @fromDate AND 
CAST(MasterCustomerPosting.createdAt AS date) &lt;= @toDate 
ORDER BY userName ASC</CommandText>
      </Query>
      <Fields>
        <Field Name="userName">
          <DataField>userName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
