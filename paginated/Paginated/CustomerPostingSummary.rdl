    <DataSet Name="MasterCustomerPosting">
      <Query>
        <DataSourceName>SqlServerPortalDw</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@postingType">
            <Value>=Parameters!postingType.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@fromDate">
            <Value>=Parameters!fromDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@toDate">
            <Value>=Parameters!toDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@userName">
            <Value>=Parameters!userName.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        RefPostingType.name AS postingType, MasterCustomerPosting.amount, MasterCustomerPosting.createdAt, MasterCustomerPosting.userName, MasterCustomerPosting.fullName, MasterCustomerPosting.batchNo, 
                         MasterCustomerPosting.refNo, MasterCustomerPosting.remarks, CONCAT(MasterCustomer.firstName, ', ', MasterCustomer.lastName) AS customerName
FROM            MasterCustomerPosting INNER JOIN
                         RefPostingType ON MasterCustomerPosting.postingTypeId = RefPostingType.id INNER JOIN
                         MasterCustomer ON MasterCustomerPosting.customerId = MasterCustomer.id  
WHERE 
MasterCustomerPosting.postingTypeId IN (@postingType)  AND 
CAST(MasterCustomerPosting.createdAt AS date) &gt;=@fromDate AND 
CAST(MasterCustomerPosting.createdAt AS date) &lt;= @toDate AND 
MasterCustomerPosting.userName IN (@userName)
ORDER BY createdAt DESC</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="postingType">
          <DataField>postingType</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="amount">
          <DataField>amount</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="createdAt">
          <DataField>createdAt</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="userName">
          <DataField>userName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="fullName">
          <DataField>fullName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="batchNo">
          <DataField>batchNo</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="refNo">
          <DataField>refNo</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="remarks">
          <DataField>remarks</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="customerName">
          <DataField>customerName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
