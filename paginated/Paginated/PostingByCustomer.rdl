    <DataSet Name="MasterCustomer">
      <Query>
        <DataSourceName>SqlServerPortalDw</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@businessCode">
            <Value>=Parameters!businessCode.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@firstName">
            <Value>=Parameters!firstName.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@lastName">
            <Value>=Parameters!lastName.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT id, CONCAT(lastName, ', ', firstName) AS fullName
FROM MasterCustomer 
WHERE businessCode = @businessCode 
AND firstName LIKE '%' + ISNULL(@firstName, '') + '%'
AND lastName LIKE '%' + ISNULL(@lastName, '') + '%'
ORDER BY lastName ASC</CommandText>
      </Query>
      <Fields>
        <Field Name="id">
          <DataField>id</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="fullName">
          <DataField>fullName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="PostingTypeList">
      <Query>
        <DataSourceName>SqlServerPortalDw</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@customerId">
            <Value>=Parameters!customerId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT DISTINCT(postingTypeId) AS value, RefPostingType.name AS label
FROM MasterCustomerPosting 
INNER JOIN RefPostingType ON RefPostingType.id = MasterCustomerPosting.postingTypeId
WHERE customerId = @customerId</CommandText>
      </Query>
      <Fields>
        <Field Name="value">
          <DataField>value</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="label">
          <DataField>label</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
