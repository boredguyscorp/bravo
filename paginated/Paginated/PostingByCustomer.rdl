    <DataSet Name="MasterCustomerPosting">
      <Query>
        <DataSourceName>SqlServerPortalDw</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@customerId">
            <Value>=Parameters!customerId.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@postingType">
            <Value>=Parameters!postingType.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT  amount, refNo, remarks, posting.createdAt, userName, fullName, postingType.name AS postingType
FROM MasterCustomerPosting AS posting
JOIN RefPostingType AS postingType ON postingType.id = posting.postingTypeId  
WHERE customerId = @customerId AND 
posting.postingTypeId  IN (@postingType)</CommandText>
      </Query>
      <Fields>
        <Field Name="amount">
          <DataField>amount</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
