    <DataSet Name="ViewMasterCustomer">
      <Query>
        <DataSourceName>SqlServerPortalDw</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@businessCode">
            <Value>=Parameters!businessCode.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@firstName">
            <Value>=Parameters!firstName.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@lastName">
            <Value>=Parameters!lastName.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@customerStatus">
            <Value>=Parameters!customerStatus.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@classId">
            <Value>=Parameters!classId.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@groupId">
            <Value>=Parameters!groupId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT * FROM ViewMasterCustomer 
WHERE 
businessCode = @businessCode 
AND firstName LIKE '%' + ISNULL(@firstName, '') + '%'
AND lastName LIKE '%' + ISNULL(@lastName, '') + '%'
AND status IN (@customerStatus)
AND classId IN (@classId)
AND groupId IN (@groupId) 
ORDER BY firstName ASC</CommandText>
      </Query>
